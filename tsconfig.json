{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"lib": ["ES2022"],
		"moduleResolution": "bundler",
		"types": ["@cloudflare/workers-types", "vitest/globals"],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*", "test/**/*"],
	"exclude": ["node_modules"]
}
